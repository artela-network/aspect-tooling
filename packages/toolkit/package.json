{
  "name": "@artela/aspect-tool",
  "version": "0.0.26",
  "description": "Dev tool for aspect to generate libs from solidity storage layout.",
  "engines": {
    "node": ">=14"
  },
  "bin": {
    "aspect-tool": "bin/run"
  },
  "main": "dist/index.js",
  "files": [
    "CHANGELOG.md",
    "bin",
    "dist",
    "oclif.manifest.json",
    "README.md",
    "!dist/*.tar.gz"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json && oclif manifest && oclif readme",
    "oclif:pack": "npm pack && pnpm oclif pack tarballs --no-xz && node scripts/rename-tarballs.mjs",
    "format": "prettier --write -c **/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@oclif/core": "2.8.8",
    "glob": "^10.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "bin": "aspect-tool",
    "commands": "./dist/commands",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "devDependencies": {
    "@oclif/test": "^2.3.26",
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-typescript": "^0.14.0",
    "oclif": "^3.9.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "typescript-eslint-parser": "^22.0.0",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "chai": "^4",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "shx": "^0.3.3"
  }
}
