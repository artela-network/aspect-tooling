      "description": "debug_getRawHeader 1",
      "account": "",
      "type": "jsonRPC",
      "options": {
        "method": "debug_getRawHeader",
        "params": ["0x1"]
      },
      "expect": {
        "result.ret.length": {
          "gt": 100
        },
        "error": {
          "eq": ""
        }
      }
    },
    {
      "description": "debug_getRawHeader latest",
      "account": "",
